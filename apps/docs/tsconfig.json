{
  "extends": "tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["*"],
      "types/*": ["types/*"],
      "public/*": ["public/*"],
      "contents/*": ["contents/*"],
      "components/*": ["components/*"],
    }
  }
}
